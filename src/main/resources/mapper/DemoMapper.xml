<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.guo.sharding.mapper.DemoMapper">

    <insert id="saveDemo">
        INSERT INTO sys_user (name)
        VALUES (#{name});
    </insert>

    <select id="getDemo" resultType="com.guo.sharding.entity.Demo">
        select name from sys_user
    </select>
</mapper>
